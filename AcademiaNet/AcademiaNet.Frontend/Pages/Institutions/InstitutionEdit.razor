@page "/institutions/edit/{Id:int}"

@if (institutionDTO is null)
{
    <Loading/>
}
else
{
    <MudDialog>
        <DialogContent>
            <InstitutionForm @ref="institutionForm" InstitutionDTO="institutionDTO" OnValidSubmit="EditAsync" ReturnAction="Return" />
        </DialogContent>
    </MudDialog>
}





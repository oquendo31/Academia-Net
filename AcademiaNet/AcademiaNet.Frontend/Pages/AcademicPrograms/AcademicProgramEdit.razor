@page "/academicPrograms/edit/{Id:int}"


@if (academicProgramDTO is null)
{
    <Loading/>
}
else
{
    <MudDialog>
        <DialogContent>
            <AcademicProgramForm @ref="academicProgramForm" AcademicProgramDTO="academicProgramDTO" OnValidSubmit="EditAsync" ReturnAction="Return" />
        </DialogContent>
    </MudDialog>
}
